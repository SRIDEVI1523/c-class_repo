ISA=RV64IMAFDC
MMU=enable
AXI=enable
BPU=disable
MUL=asic
PERF=enable
COREFABRIC=AXI4
SYNTH=SIM
VERBOSITY=0
PREFETCH=enable
DEBUG=disable
OPENOCD=disable
USERTRAPS=False
USER=True
SUPERVISOR=disable
MULSTAGES=8
DIVSTAGES=64
FPGA=xc7a100tcsg324-1
